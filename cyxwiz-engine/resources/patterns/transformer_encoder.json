{
  "id": "transformer_encoder",
  "name": "Transformer Encoder",
  "description": "Transformer encoder stack with multi-head self-attention. Suitable for classification, embedding, and feature extraction tasks.",
  "category": "Transformer",
  "tags": ["transformer", "attention", "encoder", "bert", "nlp"],
  "parameters": [
    {
      "name": "d_model",
      "type": "int",
      "default_value": "256",
      "description": "Model dimension (embedding size)",
      "min_value": "64",
      "max_value": "2048"
    },
    {
      "name": "num_heads",
      "type": "int",
      "default_value": "8",
      "description": "Number of attention heads",
      "min_value": "1",
      "max_value": "32"
    },
    {
      "name": "ff_dim",
      "type": "int",
      "default_value": "512",
      "description": "Feed-forward hidden dimension",
      "min_value": "64",
      "max_value": "4096"
    },
    {
      "name": "vocab_size",
      "type": "int",
      "default_value": "10000",
      "description": "Vocabulary size for embedding",
      "min_value": "100",
      "max_value": "100000"
    },
    {
      "name": "num_classes",
      "type": "int",
      "default_value": "2",
      "description": "Number of output classes",
      "min_value": "2",
      "max_value": "10000"
    },
    {
      "name": "dropout_rate",
      "type": "float",
      "default_value": "0.1",
      "description": "Dropout rate",
      "min_value": "0.0",
      "max_value": "0.5"
    }
  ],
  "template": {
    "nodes": [
      {"id": "input", "type": "DatasetInput", "name": "Token Input", "pos": [0, 200]},
      {"id": "embed", "type": "Embedding", "name": "Token Embedding", "pos": [200, 200], "params": {"vocab_size": "$vocab_size", "embedding_dim": "$d_model"}},
      {"id": "pos_enc", "type": "PositionalEncoding", "name": "Positional Encoding", "pos": [400, 200], "params": {"d_model": "$d_model"}},
      {"id": "dropout1", "type": "Dropout", "name": "Dropout", "pos": [600, 200], "params": {"rate": "$dropout_rate"}},
      {"id": "enc1", "type": "TransformerEncoder", "name": "Encoder Layer 1", "pos": [800, 200], "params": {"d_model": "$d_model", "num_heads": "$num_heads", "ff_dim": "$ff_dim", "dropout": "$dropout_rate"}},
      {"id": "enc2", "type": "TransformerEncoder", "name": "Encoder Layer 2", "pos": [1000, 200], "params": {"d_model": "$d_model", "num_heads": "$num_heads", "ff_dim": "$ff_dim", "dropout": "$dropout_rate"}},
      {"id": "pool", "type": "GlobalAvgPool", "name": "Global Avg Pool", "pos": [1200, 200]},
      {"id": "dense1", "type": "Dense", "name": "Dense", "pos": [1400, 200], "params": {"units": "$d_model"}},
      {"id": "act1", "type": "ReLU", "name": "ReLU", "pos": [1600, 200]},
      {"id": "dropout2", "type": "Dropout", "name": "Dropout", "pos": [1800, 200], "params": {"rate": "$dropout_rate"}},
      {"id": "output_layer", "type": "Dense", "name": "Output", "pos": [2000, 200], "params": {"units": "$num_classes"}},
      {"id": "softmax", "type": "Softmax", "name": "Softmax", "pos": [2200, 200]},
      {"id": "output", "type": "Output", "name": "Output", "pos": [2400, 200]}
    ],
    "links": [
      {"from": "input", "to": "embed"},
      {"from": "embed", "to": "pos_enc"},
      {"from": "pos_enc", "to": "dropout1"},
      {"from": "dropout1", "to": "enc1"},
      {"from": "enc1", "to": "enc2"},
      {"from": "enc2", "to": "pool"},
      {"from": "pool", "to": "dense1"},
      {"from": "dense1", "to": "act1"},
      {"from": "act1", "to": "dropout2"},
      {"from": "dropout2", "to": "output_layer"},
      {"from": "output_layer", "to": "softmax"},
      {"from": "softmax", "to": "output"}
    ]
  }
}
